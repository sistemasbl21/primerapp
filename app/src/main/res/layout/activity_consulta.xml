<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#DBDADA"
    tools:context=".ConsultaActivity">

    <TextView
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:id="@+id/nombrer"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="32dp"
        app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="32dp"
        app:layout_constraintHorizontal_bias="0.654"
        android:text="NOMBRE PROPRIETARIO"
        android:background="#E7E7E7"
        app:layout_constraintTop_toBottomOf="@+id/idreparacion"/>
    <TextView
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:id="@+id/cedular"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="32dp"
        app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="32dp"
        app:layout_constraintHorizontal_bias="0.654"
        android:text="CEDULA"
        android:background="#E7E7E7"
        app:layout_constraintTop_toBottomOf="@+id/nombrer" android:layout_marginTop="4dp"/>
    <TextView
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:id="@+id/marcar"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="32dp"
        app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="32dp"
        app:layout_constraintHorizontal_bias="0.654"
        android:text="MARCA"
        android:background="#E7E7E7"
        app:layout_constraintTop_toBottomOf="@+id/cedular" android:layout_marginTop="4dp"/>
    <TextView
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:id="@+id/estador"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="32dp"
        app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="32dp"
        app:layout_constraintHorizontal_bias="0.654"
        android:text="ESTADO EQUIPO"
        android:background="#E7E7E7"
        app:layout_constraintTop_toBottomOf="@+id/marcar" android:layout_marginTop="4dp"/>
    <TextView
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:id="@+id/valorr"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="32dp"
        app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="32dp"
        app:layout_constraintHorizontal_bias="0.654"
        android:text="VALOR"
        android:background="#E7E7E7"
        app:layout_constraintTop_toBottomOf="@+id/estador" android:layout_marginTop="4dp"/>
    <TextView
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:id="@+id/fechair"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="32dp"
        app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="32dp"
        app:layout_constraintHorizontal_bias="0.654"
        android:text="FECHA INGRESO"
        android:background="#E7E7E7"
        app:layout_constraintTop_toBottomOf="@+id/valorr" android:layout_marginTop="4dp"/>
    <TextView
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:id="@+id/fechasr"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="32dp"
        app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="32dp"
        app:layout_constraintHorizontal_bias="0.654"
        android:text="FECHA SALIDA"
        android:background="#E7E7E7"
        app:layout_constraintTop_toBottomOf="@+id/fechair" android:layout_marginTop="4dp"/>
    <TextView
        android:layout_width="300dp"
        android:layout_height="70dp"
        android:id="@+id/observar"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="32dp"
        app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="32dp"
        app:layout_constraintHorizontal_bias="0.654"
        android:text="OBSERVACIONES"
        android:background="#E7E7E7"
        app:layout_constraintTop_toBottomOf="@+id/fechasr" android:layout_marginTop="4dp"/>
    <Button
        android:text="consultar"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:id="@+id/bconsultarr"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="32dp"
        app:layout_constraintEnd_toStartOf="@+id/bconsultarrqr"
        app:layout_constraintHorizontal_bias="0.466" android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/observar"/>
    <Button
        android:text="consultar Q/R"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:id="@+id/bconsultarrqr"
        app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="32dp" android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/observar"/>
    <EditText
        android:layout_width="217dp"
        android:layout_height="45dp"
        android:ems="10"
        android:id="@+id/idreparacion"
        android:hint="*ID"
        android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
        app:layout_constraintStart_toStartOf="parent" android:inputType="number"
        android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.496"/>
</androidx.constraintlayout.widget.ConstraintLayout>